<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EDE8E8"
    tools:context=".TelamuralActivity">


    <Button
        android:id="@+id/button6"
        android:layout_width="67dp"
        android:layout_height="60dp"
        android:drawableLeft="@drawable/_564512_burger_catalogue_list_menu_icon"
        tools:layout_editor_absoluteX="2dp"
        tools:layout_editor_absoluteY="2dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="monospace"
        android:includeFontPadding="true"
        android:text="GREEN SOLVES"
        android:textColor="#57CC99"
        android:textSize="30sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="127dp"
        tools:layout_editor_absoluteY="30dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/imagemural1"
        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="98dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="105dp"
        android:layout_height="38dp"
        app:srcCompat="@drawable/_205199_dot_hide_menu_more_icon"
        tools:layout_editor_absoluteX="157dp"
        tools:layout_editor_absoluteY="271dp" />

</android.support.constraint.ConstraintLayout>
